{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "jsx": "react-jsx",
    "downlevelIteration": true
  },
  "include": ["./src"]
}
